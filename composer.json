{
  "name": "jontsa/maintenance-bundle",
  "description": "Symfony bundle to put your site in to maintenance mode.",
  "license": "MIT",
  "type": "symfony-bundle",

  "require": {
    "php": "^7.4|^8.0",
    "symfony/http-kernel": "^5.3|^6.0",
    "symfony/console": "^5.3|^6.0",
    "symfony/config": "^5.3|^6.0",
    "symfony/dependency-injection": "^5.3|^6.0"
  },
  "autoload": {
    "psr-4": {
      "Jontsa\\Bundle\\MaintenanceBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jontsa\\Bundle\\MaintenanceBundle\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  }
}
