{
  "name": "jontsa/maintenance-bundle",
  "description": "Symfony bundle to put your site in to maintenance mode.",
  "license": "MIT",
  "type": "symfony-bundle",

  "require": {
    "php": "^8.0",
    "symfony/http-kernel": "^6.0|^7.0",
    "symfony/console": "^6.0|^7.0",
    "symfony/config": "^6.0|^7.0",
    "symfony/dependency-injection": "^6.0|^7.0"
  },
  "autoload": {
    "psr-4": {
      "Jontsa\\Bundle\\MaintenanceBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jontsa\\Bundle\\MaintenanceBundle\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "phpstan/phpstan": "^1.10",
    "phpstan/phpstan-phpunit": "^1.3",
    "phpstan/phpstan-symfony": "^1.3"
  }
}
